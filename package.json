{
  "name": "@catalyst-elements/bundle",
  "version": "0.7.1",
  "description": "A bundle of all the catalyst elements.",
  "license": "BSD-3-Clause",
  "main": "src/catalyst-elements.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/catalyst/CatalystElementsBundle.git"
  },
  "author": {
    "name": "Rebecca Stevens",
    "email": "rebeccastevens@catalyst.net.nz"
  },
  "bugs": {
    "url": "https://github.com/catalyst/CatalystElementsBundle/issues"
  },
  "homepage": "https://github.com/catalyst/CatalystElementsBundle#readme",
  "keywords": [
    "web-components"
  ],
  "scripts": {
    "format": "./node_modules/.bin/prettier-eslint --write \"{{src,test,demo,tasks}/**/*.{js,css,scss},./*.{js,json,md}}\"",
    "format:check": "./node_modules/.bin/prettier-eslint --list-different \"{{src,test,demo,tasks}/**/*.{js,css,scss},./*.{js,json,md}}\"",
    "analyze": "./node_modules/.bin/gulp analyze",
    "build": "./node_modules/.bin/gulp build",
    "build-docs": "./node_modules/.bin/gulp build-docs",
    "clean": "./node_modules/.bin/gulp clean",
    "lint": "./node_modules/.bin/gulp lint",
    "serve": "./node_modules/.bin/polyserve --npm",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "./node_modules/.bin/gulp fix-dependencies",
    "prepublishOnly": "echo \"Error: must publish from dist/\" && exit 1"
  },
  "devDependencies": {
    "@catalyst-elements/collection": "latest",
    "@polymer/iron-component-page": "^3.0.0-pre.1",
    "@polymer/iron-demo-helpers": "^3.0.0-pre.1",
    "@polymer/test-fixture": "^3.0.0-pre.1",
    "@webcomponents/shadycss": "^1.1.1",
    "@webcomponents/webcomponentsjs": "^1.1.0",
    "babel-eslint": "^8.2.2",
    "eslint": "^4.19.1",
    "eslint-config-catalyst": "^1.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-promise": "^3.7.0",
    "gulp": "^4.0.0",
    "polyserve": "^0.26.0",
    "prettier": "^1.11.1",
    "prettier-eslint-cli": "^4.7.1",
    "web-component-tester": "^6.5.0"
  },
  "engines": {
    "yarn": ">= 1.0.0"
  },
  "dependencies": {
    "@catalyst-elements/catalyst-labelable-mixin": "^0.1.2",
    "@catalyst-elements/catalyst-lazy-properties-mixin": "^0.1.0"
  }
}
